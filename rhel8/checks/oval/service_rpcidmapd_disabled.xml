<def-group>

  

  <definition class="compliance" id="service_rpcidmapd_disabled" version="1">
    <metadata>
      <title>Service rpcidmapd Disabled</title>
      <affected family="unix">
        <platform>multi_platform_all</platform>
      </affected>
      <description>The rpcidmapd service should be disabled if possible.</description>
    </metadata>
    <criteria comment="package nfs-utils removed or service rpcidmapd is not configured to start" operator="OR">
      <criterion comment="nfs-utils removed" test_ref="test_service_rpcidmapd_package_nfs-utils_removed" />
      <criteria operator="AND" comment="service rpcidmapd is not configured to start">
        <criterion comment="rpcidmapd is not running" test_ref="test_service_not_running_rpcidmapd" />
        <criterion comment="Property LoadState of service rpcidmapd is masked" test_ref="test_service_loadstate_is_masked_rpcidmapd" />
        <criterion comment="Property FragmentPath of service rpcidmapd is set to /dev/null" test_ref="test_service_fragmentpath_is_dev_null_rpcidmapd" />
      </criteria>
    </criteria>
  </definition>

  <linux:systemdunitdependency_test check="all" check_existence="any_exist" comment="systemd test" id="test_rpcidmapd_not_wanted_by_multi_user_target" version="1">
    <linux:object object_ref="object_multi_user_target_for_rpcidmapd_disabled" />
    <linux:state state_ref="state_systemd_rpcidmapd_off"/>
  </linux:systemdunitdependency_test>
  <linux:systemdunitdependency_object id="object_multi_user_target_for_rpcidmapd_disabled" comment="list of dependencies of multi-user.target" version="1">
    <linux:unit>multi-user.target</linux:unit>
  </linux:systemdunitdependency_object>
  <linux:systemdunitdependency_state id="state_systemd_rpcidmapd_off" comment="rpcidmapd service is not listed in the dependencies" version="1">
    <linux:dependency entity_check="none satisfy">rpcidmapd.service</linux:dependency>
  </linux:systemdunitdependency_state>

  <linux:systemdunitdependency_test check="all" check_existence="any_exist" comment="systemd test" id="test_rpcidmapd_socket_not_wanted_by_multi_user_target" version="1">
    <linux:object object_ref="object_multi_user_target_for_rpcidmapd_socket_disabled" />
    <linux:state state_ref="state_systemd_rpcidmapd_socket_off"/>
  </linux:systemdunitdependency_test>
  <linux:systemdunitdependency_object id="object_multi_user_target_for_rpcidmapd_socket_disabled" comment="list of dependencies of multi-user.target" version="1">
    <linux:unit>multi-user.target</linux:unit>
  </linux:systemdunitdependency_object>
  <linux:systemdunitdependency_state id="state_systemd_rpcidmapd_socket_off" comment="rpcidmapd socket is not listed in the dependencies" version="1">
    <linux:dependency entity_check="none satisfy">rpcidmapd.socket</linux:dependency>
  </linux:systemdunitdependency_state>

  <linux:systemdunitproperty_test id="test_service_not_running_rpcidmapd" check="all" check_existence="any_exist" comment="Test that the rpcidmapd service is not running" version="1">
    <linux:object object_ref="obj_service_not_running_rpcidmapd"/>
    <linux:state state_ref="state_service_not_running_rpcidmapd"/>
  </linux:systemdunitproperty_test>
  <linux:systemdunitproperty_object id="obj_service_not_running_rpcidmapd" comment="Retrieve the ActiveState property of rpcidmapd" version="1">
    <linux:unit operation="pattern match">^rpcidmapd\.(service|socket)$</linux:unit>
    <linux:property>ActiveState</linux:property>
  </linux:systemdunitproperty_object>
  <linux:systemdunitproperty_state id="state_service_not_running_rpcidmapd" version="1" comment="rpcidmapd is not running">
      <linux:value>inactive</linux:value>
  </linux:systemdunitproperty_state>
  <linux:systemdunitproperty_test id="test_service_loadstate_is_masked_rpcidmapd" check="all" check_existence="any_exist" comment="Test that the property LoadState from the service rpcidmapd is masked" version="1">
    <linux:object object_ref="obj_service_loadstate_is_masked_rpcidmapd"/>
    <linux:state state_ref="state_service_loadstate_is_masked_rpcidmapd"/>
  </linux:systemdunitproperty_test>
  <linux:systemdunitproperty_object id="obj_service_loadstate_is_masked_rpcidmapd" comment="Retrieve the LoadState property of rpcidmapd" version="1">
    <linux:unit operation="pattern match">^rpcidmapd\.(service|socket)$</linux:unit>
    <linux:property>LoadState</linux:property>
  </linux:systemdunitproperty_object>
  <linux:systemdunitproperty_state id="state_service_loadstate_is_masked_rpcidmapd" version="1" comment="LoadState is set to masked">
      <linux:value>masked</linux:value>
  </linux:systemdunitproperty_state>

  <linux:systemdunitproperty_test id="test_service_fragmentpath_is_dev_null_rpcidmapd" check="all" check_existence="any_exist" comment="Test that the property FragmentPath from the service rpcidmapd is set to /dev/null" version="1">
    <linux:object object_ref="obj_service_fragmentpath_is_dev_null_rpcidmapd"/>
    <linux:state state_ref="state_service_fragmentpath_is_dev_null_rpcidmapd"/>
  </linux:systemdunitproperty_test>
  <linux:systemdunitproperty_object id="obj_service_fragmentpath_is_dev_null_rpcidmapd" comment="Retrieve the FragmentPath property of rpcidmapd" version="1">
    <linux:unit operation="pattern match">^rpcidmapd\.(service|socket)$</linux:unit>
    <linux:property>FragmentPath</linux:property>
  </linux:systemdunitproperty_object>
  <linux:systemdunitproperty_state id="state_service_fragmentpath_is_dev_null_rpcidmapd" version="1" comment="FragmentPath is set to /dev/null">
      <linux:value>/dev/null</linux:value>
  </linux:systemdunitproperty_state>





  <linux:rpminfo_test check="all" check_existence="none_exist"
  id="test_service_rpcidmapd_package_nfs-utils_removed" version="1"
  comment="package nfs-utils is removed">
    <linux:object object_ref="obj_test_service_rpcidmapd_package_nfs-utils_removed" />
  </linux:rpminfo_test>
  <linux:rpminfo_object id="obj_test_service_rpcidmapd_package_nfs-utils_removed" version="1">
    <linux:name>nfs-utils</linux:name>
  </linux:rpminfo_object>


</def-group>