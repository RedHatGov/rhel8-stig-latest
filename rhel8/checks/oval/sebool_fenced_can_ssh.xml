<def-group>
  <definition class="compliance" id="sebool_fenced_can_ssh" version="1">
    <metadata>
      <title>SELinux "fenced_can_ssh" Boolean Check</title>
      <affected family="unix">
        <platform>multi_platform_rhel</platform>
        <platform>multi_platform_fedora</platform>
        <platform>multi_platform_rhv</platform>
        <platform>multi_platform_ol</platform>
      </affected>
      <description>The SELinux "fenced_can_ssh" boolean should be set in the system configuration.</description>
    </metadata>
    <criteria>
      <criterion comment="fenced_can_ssh is configured correctly" test_ref="test_sebool_fenced_can_ssh" />
    </criteria>
  </definition>

  <linux:selinuxboolean_test check="all" check_existence="all_exist" comment="fenced_can_ssh is configured correctly" id="test_sebool_fenced_can_ssh" version="1">
    <linux:object object_ref="object_sebool_fenced_can_ssh" />
    <linux:state state_ref="state_sebool_fenced_can_ssh" />
  </linux:selinuxboolean_test>

  <linux:selinuxboolean_object id="object_sebool_fenced_can_ssh" version="1">
    <linux:name>fenced_can_ssh</linux:name>
  </linux:selinuxboolean_object>

  <linux:selinuxboolean_state id="state_sebool_fenced_can_ssh" version="1">
    <linux:name>fenced_can_ssh</linux:name>
    
      <linux:current_status datatype="boolean" var_ref="var_fenced_can_ssh"/>
      <linux:pending_status datatype="boolean" var_ref="var_fenced_can_ssh"/>
    
  </linux:selinuxboolean_state>

  
    <external_variable comment="external variable for fenced_can_ssh" datatype="boolean" id="var_fenced_can_ssh" version="1" />
  
</def-group>